[test_case]
module = perfrunner.tests.sync_gateway
class = GateloadTest
title = Gateload test for Sync Gateway performance
summary = 2 Sync Gateways in parallel, each with 5K users, compression ON, conn_db = 16, conn_in = 0
use_workers = 0

[cluster]
mem_quota = 26624
initial_nodes = 3
num_buckets = 1

[bucket]
replica_number = 1

[gateway]
compression = true
conn_db = 16
conn_in = 0

[gateload]
rampup_interval = 900
run_time = 3600
pushers = 1500
pullers = 3500
doc_size = 0
p95_avg_criteria = 3
p99_avg_criteria = 5

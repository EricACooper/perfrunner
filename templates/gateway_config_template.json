{
        "log": ["HTTP"],
        "adminInterface": ":4985",
        "maxIncomingConnections": "{{conn_in}}",
        "maxCouchbaseConnections": "{{conn_db}}",
        "MaxFileDescriptors": 90000,  
        "SlowServerCallWarningThreshold": 500,
        "CompressResponses": {{compression}},
        "databases": {
                "db": {
                        "server": "http://{{bucket}}:password@{{db_master}}/",
                        "bucket": "{{bucket}}",
                        "users": {
                                "GUEST": { "disabled": true }
                        }
                }
        }
}
